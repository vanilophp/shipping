{
    "name": "vanilo/shipping",
    "description": "Vanilo Shipping Module",
    "type": "library",
    "license": "MIT",
    "keywords": ["shipping", "ecommerce", "vanilo", "artkonekt", "laravel"],
    "support": {
        "source": "https://github.com/vanilophp/shipping"
    },
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "konekt/concord": "^1.6",
        "konekt/address": "^2.1"

    },
    "require-dev": {
        "orchestra/testbench": "~4.0|~5.0|~6.0",
        "phpunit/phpunit" : "~8.0|~9.0"
    },
    "autoload": {
        "psr-4": { "Vanilo\\Shipping\\": "src/" }
    },
    "autoload-dev": {
